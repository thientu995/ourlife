{
  "hosting": {
    "public": "docs",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**/*",
        "function": "app"
      },
      {
        "source": "/**",
        "dynamicLinks": true
      }
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=7200"
          }
        ]
      }
    ]
  },
  "functions": {
    "predeploy": [
      "node ./index.js"
    ],
    "source": "./"
  }
}