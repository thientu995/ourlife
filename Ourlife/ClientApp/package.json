{
  "name": "client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "echo Starting && ng serve --no-live-reload",
    "build": "ng build --prod --aot --optimization --build-optimizer --vendor-chunk --common-chunk --extract-licenses --extract-css --source-map=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "create": "ng g c -n --module=app",
    "dev:ssr": "ng run ClientApp:serve-ssr",
    "serve:ssr": "node dist/ClientApp/server/main.js",
    "build:ssr": "npm run build && ng run ClientApp:server:production",
    "prerender": "ng run ClientApp:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/forms": "^9.1.13",
    "@angular/localize": "^9.1.13",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/platform-server": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@angular/service-worker": "^11.2.6",
    "@nguniversal/express-engine": "^11.0.1",
    "aspnet-prerendering": "^3.0.1",
    "express": "^4.15.2",
    "flickr-justified-gallery": "^1.0.7",
    "material-icons": "^0.3.1",
    "ng-lazyload-image": "^9.1.0",
    "ngx-pinch-zoom": "^2.5.4",
    "rxjs": "~6.5.4",
